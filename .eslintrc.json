{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime"
    ],
    "settings": {
      "react":{
        "version": "detect"
      }
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json",
        "warnOnUnsupportedTypeScriptVersion": false
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "comma-dangle": "off"
    }
}
